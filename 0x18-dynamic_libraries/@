#include <stdio.h>
#include <stdlib.h>

/**
 * add - add two numbers
 * @a: first number
 * @b: second number
 * Return: always interger
 */

int add(int a, int b)
{
	int result;

	result = a + b;
	return (result);
}

/**
 * mul - multiplies two numbers
 * @a: first number
 * @b: second number
 * Return: always interger
 */

int mul(int a, int b)
{
        int result;

        result = a * b;
        return (result);
}

/**
 * sub - subtract two numbers
 * @a: first number
 * @b: second number
 * Return: always interger
 */

int sub(int a, int b)
{
        int result;

        result = a - b;
        return (result);
}

/**
 * div - dividies two numbers
 * @a: first number
 * @b: second number
 * Return: always interger
 */

int div(int a, int b)
{
        float result;

        result = a / b;
        return (result);
}

/**
 * mod - find mode of a numbers
 * @a: first number
 * @b: second number
 * Return: always interger
 */

int mod(int a, int b)
{
        int result;

        result = a % b;
        return (result);
}
